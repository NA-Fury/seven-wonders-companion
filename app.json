{
  "expo": {
    "name": "seven-wonders-companion",
    "slug": "seven-wonders-companion",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sevenwonderscompanion",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#F5F1D0",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.na.fury.wonderscompanion",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F5F1D0"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      ["@sentry/react-native/expo", { "organization": "gmv-ib", "project": "seven-wonders-companion" }],
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "sentryDsn": "https://<publicKey>@o<orgId>.ingest.sentry.io/<projectId>",
      "eas": {
        "projectId": "56d5d048-d6e6-46e6-a522-e9c6c7cb9f7f"
      },
      "router": {}
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "na-fury"
  }
}
