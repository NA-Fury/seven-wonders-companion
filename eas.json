{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",          
        "releaseStatus": "draft"  
      },
      "ios": {
        "ascAppId": "YOUR_ASC_APP_ID_IF_KNOWN" 
      }
    }
  }
}
