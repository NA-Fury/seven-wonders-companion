{
  "name": "seven-wonders-companion",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:lan": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "doctor": "expo-doctor",
    "install:check": "expo install --check",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "clean": "expo r -c",
    "prebuild": "expo prebuild --clean",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "cross-env EXPO_USE_STATIC=1 jest",
    "test:once": "cross-env EXPO_USE_STATIC=1 jest --watchAll=false",
    "test:watch": "cross-env EXPO_USE_STATIC=1 jest --watch",
    "test:ci": "cross-env EXPO_USE_STATIC=1 jest --ci --reporters=default",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm test"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "~6.14.0",
    "babel-preset-expo": "~13.0.0",
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "immer": "^10.1.3",
    "lucide-react-native": "^0.540.0",
    "nativewind": "^4.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "zod": "^4.0.17",
    "zustand": "^5.0.8",
    "expo-application": "~6.1.5",
    "expo-updates": "~0.28.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-flow": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "cross-env": "^10.0.0",
    "react-test-renderer": "19.0.0",
    "schema-utils": "^4.2.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest.env.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|expo-modules-core|immer|zustand)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "private": true,
  "engines": {
    "node": ">=20.18.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NA-Fury/seven-wonders-companion.git"
  },
  "bugs": {
    "url": "https://github.com/NA-Fury/seven-wonders-companion/issues"
  },
  "homepage": "https://github.com/NA-Fury/seven-wonders-companion#readme",
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-test-renderer": "19.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "immer"
        ]
      }
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
